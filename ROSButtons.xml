<controls>
    <button>
        <title>Up</title>
        <op>publish</op>
        <topic>/turtle1/command_velocity</topic>
        <msg>
            <linear>2.0</linear>
            <angular>0.0</angular>
        </msg>
    </button>
    <button>
        <title>Left</title>
        <op>publish</op>
        <topic>/turtle1/command_velocity</topic>
        <msg>
            <linear>0.0</linear>
            <angular>1.8</angular>
        </msg>
    </button>
    <button>
        <title>Right</title>
        <op>publish</op>
        <topic>/turtle1/command_velocity</topic>
        <msg>
            <linear>0.0</linear>
            <angular>-1.8</angular>
        </msg>
    </button>
    <button>
        <title>Down</title>
        <op>publish</op>
        <topic>/turtle1/command_velocity</topic>
        <msg>
            <linear>-2.0</linear>
            <angular>0.0</angular>
        </msg>
    </button>
</controls>
